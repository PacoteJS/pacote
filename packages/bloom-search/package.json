    "watch": "concurrently 'pnpm watch:cjs' 'pnpm watch:esm'",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json",
    "watch:esm": "tsc -w -p ./tsconfig.esm.json",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@pacote/array": "file:../array",
    "@pacote/bloom-filter": "file:../bloom-filter",
    "@pacote/memoize": "file:../memoize",
